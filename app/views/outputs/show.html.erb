<div class="container mt-5">
  <h1 class="display-1">Output</h1>
  <div class="col-12 card bg-light p-3 rounded min-output">
  <h4>
    <%= @output.content %>
    </h4>
  </div>
  <div class="d-flex gap-3 mt-3">
    <%= link_to "Copy", output_path, class: 'btn btn-primary'%>
    <%= link_to 'Edit', edit_output_path(@output), class: 'btn my-btn-light' %>
  </div>
</div>
